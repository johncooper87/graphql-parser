{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run",
      "args": [
        "${file}"
      ],
      "runtimeArgs": [
        "-r", "ts-node/register"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceFolder}/node_modules/**/*.js"
      ]
    }
  ]
}